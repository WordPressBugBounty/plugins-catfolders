{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "catfolders/image-gallery",
  "version": "0.1.0",
  "title": "CatFolders Image Gallery",
  "category": "media",
  "icon": "",
  "description": "Show catfolders gallery",
  "attributes": {
    "folders": {
      "type": "array",
      "default": []
    },
    "layout": {
      "type": "string",
      "default": "grid"
    },
    "columns": {
      "type": "number",
      "default": 3
    },
    "caption": {
      "type": "boolean",
      "default": false
    },
    "enableLightbox": {
      "type": "boolean",
      "default": false
    },
    "thumbnailLink": {
      "type": "string",
      "default": "none"
    },
    "thumbnailLinkTarget": {
      "type": "string",
      "default": "_self"
    }
  },
  "textdomain": "catfolders",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}